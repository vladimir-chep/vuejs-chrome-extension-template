{
    "name": "vuejs-chrome-extension-template",
    "description": "A simple chrome extension template with vue.js",
    "version": "0.1",
    "manifest_version": 2,
    "permissions": ["storage"],
    "background": {
        "scripts": ["background.js"]
    },
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "icons/icon-16.png",
            "32": "icons/icon-32.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        },
        "default_title": "Default Title",
        "default_popup": "popup/popup.html"
    }
}